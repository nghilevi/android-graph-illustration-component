<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.graphandphototest.app" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.github.graphandphototest.GoogleChartTools.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="aFreeChart.TruitonMainActivity"
            android:label="@string/title_activity_truiton_main" >
        </activity>
        <activity
            android:name="aChartEngine.TruitonAChartEngineActivity"
            android:label="@string/title_activity_truiton_achart_engine" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>
